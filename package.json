{
  "name": "auth",
  "version": "1.0.0",
  "description": "NestJS authentication",
  "author": "Denzel Giraldo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "web-push-keys": "web-push generate-vapid-keys",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker": "npm run docker:build; npm run docker:up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-socket.io": "^7.6.18",
    "@nestjs/websockets": "^7.6.18",
    "apple-signin-auth": "^1.7.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.41.0",
    "code-config": "^1.0.55",
    "dotenv": "^10.0.0",
    "facebook-auth-nestjs": "^0.0.3",
    "firebase-admin": "^9.12.0",
    "fs": "^0.0.1-security",
    "googleapis": "^82.0.0",
    "handlebars": "^4.7.8",
    "mongoose": "^5.13.21",
    "nodemailer": "^6.9.7",
    "qs": "^6.11.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "socket.io": "^4.7.2",
    "socket.io-redis": "^6.1.1",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.10",
    "@types/node": "^13.13.52",
    "@types/passport": "^1.0.15",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/qs": "^6.9.10",
    "@types/redis": "^2.8.32",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.16",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^3.9.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
